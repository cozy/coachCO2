{
  "name": "coachco2",
  "version": "0.10.0",
  "scripts": {
    "analyze": "COZY_SCRIPTS_ANALYZER=true yarn build",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "cs lint '{src,test}/**/*.{js,jsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "prebuild": "yarn lint",
    "build": "yarn build:browser",
    "build:browser": "cs build --browser",
    "build:mobile": "cs build --mobile",
    "watch": "yarn watch:browser",
    "watch:browser": "cs watch --browser",
    "watch:mobile": "cs watch --mobile",
    "start": "cs start --browser",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/cozy/coachCO2.git}",
    "test": "cs test --verbose --coverage",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --prepublish downcloud --postpublish mattermost --space larochelle",
    "stack:docker:dev": "cs stackDocker",
    "stack:docker:prod": "cs stackDocker --prod",
    "fixtures": "ACH import accounts/tracemob.json && ACH script timeseries/importGeojson",
    "fixtures:drop": "ACH drop io.cozy.timeseries.geojson",
    "service:timeseriesWithoutAggregateMigration": "yarn run cozy-konnector-dev -m build/manifest.webapp build/services/timeseriesWithoutAggregateMigration/coachco2.js",
    "scripts:addTrip": "node scripts/save-random-trip.js",
    "scripts:removePurposes": "node scripts/remove-purposes.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/coachCO2.git"
  },
  "author": "",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy/coachCO2/issues"
  },
  "homepage": "https://github.com/cozy/coachCO2#README",
  "devDependencies": {
    "@testing-library/jest-dom": "5.16.2",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "^7.0.2",
    "babel-preset-cozy-app": "^2.0.2",
    "bundlemon": "1.3.1",
    "cozy-ach": "^1.39.0",
    "cozy-bar": "7.16.0",
    "cozy-jobs-cli": "1.16.3",
    "cozy-scripts": "^6.3.3",
    "eslint-config-cozy-app": "^4.1.1",
    "mockdate": "^3.0.5",
    "react-test-renderer": "^17.0.2"
  },
  "dependencies": {
    "@material-ui/core": "4",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "chart.js": "3.7.1",
    "cozy-client": "^33.0.0",
    "cozy-device-helper": "^2.1.0",
    "cozy-flags": "^2.8.7",
    "cozy-intent": "^1.17.1",
    "cozy-logger": "^1.9.0",
    "cozy-realtime": "^4.2.1",
    "cozy-ui": "^73.2.0",
    "date-fns": "^2.28.0",
    "humanize-duration": "3.27.1",
    "leaflet": "^1.7.1",
    "lodash": "^4.17.21",
    "react": "17.0.2",
    "react-chartjs-2": "4.0.1",
    "react-dom": "17.0.2",
    "react-leaflet": "2.8.0",
    "react-router-dom": "6.3.0"
  }
}
