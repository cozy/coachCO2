{
  "name": "coachco2",
  "version": "0.18.0",
  "engines": {
    "node": "~16 || ~20"
  },
  "scripts": {
    "analyze": "COZY_SCRIPTS_ANALYZER=true yarn build",
    "lint": "yarn lint:js && yarn lint:styles",
    "lint:js": "cs lint '{src,test}/**/*.{js,jsx,ts,tsx}'",
    "lint:styles": "stylint src/styles --config ./.stylintrc",
    "prebuild": "yarn lint",
    "build": "yarn build:browser",
    "build:browser": "cs build --browser  --barV7 false",
    "build:mobile": "cs build --mobile  --barV7 false",
    "watch": "yarn watch:browser",
    "watch:browser": "cs watch --browser --barV7 false",
    "watch:mobile": "cs watch --mobile  --barV7 false",
    "start": "cs start --browser  --barV7 false",
    "deploy": "git-directory-deploy --directory build/ --branch ${DEPLOY_BRANCH:-build} --repo=${DEPLOY_REPOSITORY:-https://$GITHUB_TOKEN@github.com/cozy/coachCO2.git}",
    "test": "env NODE_ENV=test cs test --verbose --coverage",
    "cozyPublish": "cozy-app-publish --token $REGISTRY_TOKEN --prepublish downcloud --postpublish mattermost --space larochelle",
    "stack:docker:dev": "cs stackDocker",
    "stack:docker:prod": "cs stackDocker --prod",
    "fixtures": "ACH import accounts/tracemob.json && ACH script timeseries/importGeojson",
    "fixtures:drop": "ACH drop io.cozy.timeseries.geojson",
    "service:timeseriesWithoutAggregateMigration": "yarn run cozy-konnector-dev -m build/manifest.webapp build/services/timeseriesWithoutAggregateMigration/coachco2.js",
    "scripts:addTrip": "node scripts/save-random-trip.js",
    "scripts:removePurposes": "node scripts/remove-purposes.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cozy/coachCO2.git"
  },
  "author": "",
  "contributors": [],
  "license": "AGPL-3.0",
  "bugs": {
    "url": "https://github.com/cozy/coachCO2/issues"
  },
  "homepage": "https://github.com/cozy/coachCO2#README",
  "devDependencies": {
    "@babel/eslint-parser": "^7.22.11",
    "@babel/polyfill": "^7.12.1",
    "@babel/runtime": "^7.22.15",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@testing-library/react-hooks": "8.0.1",
    "@typescript-eslint/eslint-plugin": "6.4.1",
    "@typescript-eslint/parser": "6.4.1",
    "babel-preset-cozy-app": "^2.1.0",
    "bundlemon": "1.3.1",
    "cozy-ach": "^1.49.0",
    "cozy-jobs-cli": "1.19.2",
    "cozy-scripts": "^8.2.0",
    "eslint": "8.48.0",
    "eslint-config-cozy-app": "6.1.0",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-import": "2.28.1",
    "eslint-plugin-jest": "27.2.3",
    "eslint-plugin-prettier": "5.0.0",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.33.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "mockdate": "^3.0.5",
    "prettier": "^3.0.2",
    "prop-types": "15.8.1",
    "react-test-renderer": "18.2.0",
    "terser-webpack-plugin": "1.4.4",
    "typescript": "4.9.5",
    "react-hot-loader": "^4.13.0"
  },
  "dependencies": {
    "@sentry/integrations": "7.88.0",
    "@sentry/react": "7.88.0",
    "@react-pdf/renderer": "3.1.12",
    "chart.js": "3.7.1",
    "classnames": "2.3.2",
    "copy-text-to-clipboard": "3.2.0",
    "cozy-client": "^45.1.0",
    "cozy-device-helper": "^3.0.0",
    "cozy-flags": "^3.0.1",
    "cozy-harvest-lib": "^18.2.1",
    "cozy-intent": "^2.15.0",
    "cozy-keys-lib": "^6.0.0",
    "cozy-logger": "^1.10.4",
    "cozy-minilog": "^3.3.1",
    "cozy-realtime": "^5.0.0",
    "cozy-sharing": "^10.0.0",
    "cozy-ui": "^101.2.0",
    "date-fns": "2.29.3",
    "humanize-duration": "3.27.1",
    "leaflet": "1.9.1",
    "lodash": "^4.17.21",
    "node-fetch": "2.6.7",
    "react": "18.2.0",
    "react-chartjs-2": "4.0.1",
    "react-dom": "18.2.0",
    "react-leaflet": "4.1.0",
    "react-router-dom": "6.3.0"
  }
}
